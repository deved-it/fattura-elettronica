{
    "name": "deved/fattura-elettronica",
    "description": "la libreria definitiva per la creazione delle fatture elettroniche italiane",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Salvatore Guarino",
            "email": "sg@deved.it",
            "homepage": "http://www.deved.it"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Deved\\FatturaElettronica\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deved\\FatturaElettronica\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-xmlwriter": "*",
        "ext-libxml": "*",
        "ext-dom": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
